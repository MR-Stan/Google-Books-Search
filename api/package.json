{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "start http://localhost:5000 & npm run dev",
    "backend": "node server.js",
    "frontend": "npm run start --prefix ../client",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "build": "cd ../client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Mike Stanley",
  "devDependencies": {
    "concurrently": "^4.1.0"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "mongoose": "^5.3.16",
    "path": "^0.12.7"
  }
}
