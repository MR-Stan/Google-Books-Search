{
  "name": "google-books-search",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd ./api && node server.js",
    "start:dev": "concurrently \"node server.js\" \"npm run client\"",
    "client": "cd ./client && npm run start",
    "seed": "node scripts/seedsDB.js",
    "install": "cd ./client && npm install",
    "build": "cd ./client && npm run build",
    "heroku-postbuild": "cd ./client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MR-Stan/Google-Books-Search.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MR-Stan/Google-Books-Search/issues"
  },
  "homepage": "https://github.com/MR-Stan/Google-Books-Search#readme",
  "dependencies": {
    "concurrently": "^5.1.0",
    "if-env": "^1.0.4"
  }
}
